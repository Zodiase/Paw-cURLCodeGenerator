curl -X "{{{request.method}}}" "{{{request.url}}}" \
	{{#headers.has_headers}}
	{{#headers.header_list}}
	-H "{{{header_name}}}: {{{header_value}}}" \
	{{/headers.header_list}}
	{{/headers.has_headers}}
	{{#body.has_url_encoded_body}}
	{{#body.url_encoded_body}}
	-d "{{{name}}}={{{value}}}" \
	{{/body.url_encoded_body}}
	{{/body.has_url_encoded_body}}
	{{#body.has_raw_body}}
	-d "{{{body.raw_body}}}" \
	{{/body.has_raw_body}}
	{{#body.has_long_body}}
	-d "{set your body string}" \
	{{/body.has_long_body}}
	{{#body.has_multipart_body}}
	{{#body.multipart_body}}
	-F "{{{name}}}={{{value}}}" \
	{{/body.multipart_body}}
	{{/body.has_multipart_body}}
